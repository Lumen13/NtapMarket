@model List<NtapMarket.Data.ObjectModel.ProductModel>

<h1>Товары</h1>
<h4><br>Id продавца: 1</h4>

@{
    foreach (NtapMarket.Data.DBModel.Product item in Model)
    {
        <hr>
        <p>
            <details>
                <summary><strong>@item.Name</strong></summary>
                <br>Категория продукта: @item.ProductCategoryId
                <br>Цена: @item.Price рублей
                <br>Кол-во: @item.Count шт.
                <br><br><a href="https://localhost:44392/Product/@item.Id">Подробнее</a>
            </details>
        </p>
    }
}

